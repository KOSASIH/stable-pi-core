# Microsoft Bot Framework Configuration
MICROSOFT_APP_ID=12345678-1234-1234-1234-123456789abc
MICROSOFT_APP_PASSWORD=YourSuperSecretPassword123!

# Bot Endpoint
BOT_ENDPOINT=https://yourbot.azurewebsites.net/api/messages

# LUIS Configuration
LUIS_APP_ID=abcdefgh-ijkl-mnop-qrst-uvwxyz123456
LUIS_ENDPOINT_KEY=YourLuisEndpointKey123!
LUIS_ENDPOINT=https://westus.api.cognitive.microsoft.com/luis/v2.0/apps/${LUIS_APP_ID}?subscription-key=${LUIS_ENDPOINT_KEY}

# QnA Maker Configuration
QNA_KB_ID=12345678-1234-1234-1234-123456789abc
QNA_ENDPOINT_KEY=YourQnAEndpointKey123!
QNA_ENDPOINT=https://your-qna-service.azurewebsites.net/qnamaker/knowledgebases/${QNA_KB_ID}/generateAnswer

# Cognitive Services Configuration
SPEECH_SERVICE_KEY=YourSpeechServiceKey123!
SPEECH_SERVICE_REGION=westus

# Database Connection String
DB_SERVER=your-database-server.database.windows.net
DB_DATABASE=YourDatabaseName
DB_USER=YourDatabaseUser 
DB_PASSWORD=YourDatabasePassword123!

# Application Settings
APP_ENVIRONMENT=production
ENABLE_DEBUGGING=false
MAX_RETRIES=5
TIMEOUT=30000
ENABLE_TELEMETRY=true

# Security Settings
REQUIRE_HTTPS=true
ALLOWED_ORIGINS=https://your-allowed-origin.com
