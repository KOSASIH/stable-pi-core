# Database Configuration
DATABASE_URL=sqlite:///path/to/your/database.db  # For SQLite
# DATABASE_URL=postgresql://user:password@localhost:5432/mydatabase  # For PostgreSQL
# DATABASE_URL=mysql://user:password@localhost:3306/mydatabase  # For MySQL

# API Keys
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here  # Example for stock market data
# Add other API keys as needed

# Machine Learning Model Configuration
MODEL_PATH=models/price_prediction_model.pkl

# Flask Configuration (if using Flask for API)
FLASK_ENV=development
FLASK_APP=api.py  # Main API file
SECRET_KEY=your_secret_key_here

# Logging Configuration
LOG_LEVEL=DEBUG  # Set to INFO or ERROR in production

# Other Configuration
DEBUG=True  # Set to False in production
